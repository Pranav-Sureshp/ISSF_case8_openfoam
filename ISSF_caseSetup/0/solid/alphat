FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

alphat          4.87e-06 ; // alphaf = 19.0/(7800*500)

internalField   uniform $alphat;

boundaryField
{

     solid_inlet
    {
        type            zeroGradient;
    }
    solid_outlet
    {
        type            zeroGradient;
    }
    heater
    {
        type            zeroGradient;
    }

    exterior
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type            zeroGradient;
    }
 
}


