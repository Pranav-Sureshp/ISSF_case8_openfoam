FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions       [0 0 0 1 0 0 0];

Tw  573.0 ;

internalField   uniform $Tw;

