FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions       [0 0 0 1 0 0 0];

Tw  573.0 ;

internalField   uniform $Tw;

boundaryField
{

     solid_inlet
    {
        type            zeroGradient;
    }
    solid_outlet
    {
        type            zeroGradient;
    }
    heater
    {
        type            fixedGradient;
	gradient        -1960.8 ;      // -q/k_w = (0.04e+06 / 20.40 )
    }
    exterior
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
       type                solidWallMixedTemperatureCoupled; // solidWallMixedTemperatureCoupled
       neighbourFieldName          T;                                //
       K                          K;
       value                uniform $Tw ;
    }
   

}



