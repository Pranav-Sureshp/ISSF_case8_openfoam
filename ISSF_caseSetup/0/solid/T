{

     solid_inlet
    {
        type            zeroGradient;
    }
    solid_outlet
    {
        type            zeroGradient;
    }
    heater
    {
        type            fixedGradient;
	gradient   -qw/k ;
    }

    exterior
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type        zeroGradient; // new BC
    }
   

}



