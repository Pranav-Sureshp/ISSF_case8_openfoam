FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      sigma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 1 -3 -1 0 0 0 ];

K           19.0 ;

internalField   uniform $K ;

boundaryField
{
    fluid_inlet
    {
        type            zeroGradient;
    }
    fluid_outlet
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            fixedValue;
        value           uniform $K ;
    }

    
}


