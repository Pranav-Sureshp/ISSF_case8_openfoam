FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions       [0 0 0 1 0 0 0];


internalField   uniform 573.0 ;


boundaryField
{
    fluid_inlet
    {
        type        fixedValue;
        value       uniform 573.0 ;
    }

    fluid_outlet
    {
        type       zeroGradient;
    }

    fluid_to_solid
    {
//         type       zeroGradient;
       type            solidWallMixedTemperatureCoupled; // solidWallMixedTemperatureCoupled
       Tnbr            T;                                //
       K               K;
       value           uniform 573.0 ;
    }
}



