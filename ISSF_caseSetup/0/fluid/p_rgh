FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 4435.0 ;  // rho*g*2*a=9838.01*9.8*2*0.023

boundaryField
{
    fluid_outlet
    {
        type            fixedValue;
        value           uniform 4435.0 ;
    }
    fluid_inlet
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            zeroGradient;
    }
}

